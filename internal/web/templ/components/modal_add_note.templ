package components

templ ModalAddNote() {
    <dialog open id="dialog">
        <form
            hx-post="/notes"
            hx-target="#notes"
            hx-target-error="#error"
            hx-swap="outerHTML"
            hx-indicator=".htmx-indicator"
            hx-on::before-request="clearError()"
        >
            @ModalForm("Add note", "Creating Note...", "Create", "", "")
        </form>
    </dialog>
}
